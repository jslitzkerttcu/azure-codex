# Environment variable examples for OpenAI and Azure OpenAI Service

# Public OpenAI API
OPENAI_API_KEY=your_openai_api_key
OPENAI_BASE_URL=https://api.openai.com/v1

# Azure OPENAI Service (optional)
# Set these variables to connect to your Azure OpenAI Service resource
AZURE_OPENAI_ENDPOINT=https://<your-resource>.openai.azure.com/responses  # Make sure this points to the responses api and not the "old" completions api
AZURE_OPENAI_API_KEY=your_azure_openai_api_key
AZURE_OPENAI_API_VERSION=2025-03-01-preview  # Minimal version needed for the responses API is 2025-03-01-preview
AZURE_OPENAI_DEPLOYMENT_NAME=<your-deployment-name>